     "output_type": "display_data"
    }
   ],
   "source": [
    "x_mam = (x**2).sum()\n",
    "x_y = np.dot(x.T,y)\n",
    "a = x_y/x_mam\n",
    "new_y = a*x\n",
    "plt.plot(x,new_y)\n",
    "plt.scatter(x,y)\n",
    "plt.show()"
   ]
